.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}
.w-93{width: 93%; margin: 0 auto;}
.c-this1{ color: #ea9c16;}
