.f-32 { font-size: 32rpx; }
.f-34 { font-size: 34rpx; }
.f-36 { font-size: 36rpx; }
.f-38 { font-size: 38rpx; }
.f-40 { font-size: 40rpx; }
.f-42 { font-size: 42rpx; }
.f-44 { font-size: 44rpx; }
